{% block sw_search_bar_item_cms_page %}
    {% parent %}

    <router-link v-else-if="type === 'cbax_lexicon_entry'"
                 v-bind:to="{ name: 'cbax.lexicon.detail', params: { id: item.id } }"
                 ref="routerLink"
                 class="sw-search-bar-item__link">
        {% block sw_search_bar_item_foo_bar_label %}
            <span class="sw-search-bar-item__label">
                <sw-highlight-text v-bind:searchTerm="searchTerm"
                                   v-bind:text="item.translated.title + ' - ' + item.translated.keyword">
                </sw-highlight-text>
            </span>
        {% endblock %}
    </router-link>
{% endblock %}
