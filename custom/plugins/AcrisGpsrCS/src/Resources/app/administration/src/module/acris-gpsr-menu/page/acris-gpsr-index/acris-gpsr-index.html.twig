{% block acris_gpsr_index %}
<sw-page class="acris-gpsr-index sw-settings-index">
    {% block acris_gpsr_index_smart_bar_header %}
    <template #smart-bar-header>
        {% block acris_gpsr_index_smart_bar_header_title %}
        <h2>
            {% block acris_gpsr_index_smart_bar_header_title_text %}
                {{ $tc('acris-gpsr-menu.index.textHeadline') }}
            {% endblock %}
        </h2>
        {% endblock %}
    </template>
    {% endblock %}

    {% block acris_gpsr_index_content %}
    <template #content>
        {% block acris_gpsr_index_content_card_view %}
        <sw-card-view>
            {% block acris_gpsr_index_content_card %}
            <sw-card class="acris-gpsr-list_index__card" position-identifier="acris-gpsr-list-index">
                {% block acris_gpsr_index_content_grid %}
                <div class="acris-settings-content-grid sw-settings__content-grid sw-settings__tabs">
                    {% block acris_gpsr_index_card_item %}
                        {% block acris_gpsr_manufacturer %}
                            <sw-settings-item
                                    :label="$tc('acris-gpsr-manufacturer.general.mainMenuItemGeneral')"
                                    :to="{ name: 'acris.gpsr.manufacturer.index' }"
                                    class="acris-settings-item">
                                <template #icon>
                                    <sw-icon name="regular-home"></sw-icon>
                                </template>
                            </sw-settings-item>
                        {% endblock %}
                        {% block acris_gpsr_contact %}
                            <sw-settings-item
                                    :label="$tc('acris-gpsr-contact.general.mainMenuItemGeneral')"
                                    :to="{ name: 'acris.gpsr.contact.index' }"
                                    class="acris-settings-item">
                                <template #icon>
                                    <sw-icon name="regular-user"></sw-icon>
                                </template>
                            </sw-settings-item>
                        {% endblock %}
                        {% block acris_gpsr_note %}
                            <sw-settings-item
                                    :label="$tc('acris-gpsr-note.general.mainMenuItemGeneral')"
                                    :to="{ name: 'acris.gpsr.note.index' }"
                                    class="acris-settings-item">
                                <template #icon>
                                    <sw-icon name="regular-exclamation-triangle"></sw-icon>
                                </template>
                            </sw-settings-item>
                        {% endblock %}
                    {% endblock %}
                </div>
                {% endblock %}
            </sw-card>
            {% endblock %}
        </sw-card-view>
        {% endblock %}
    </template>
    {% endblock %}
</sw-page>
{% endblock %}
