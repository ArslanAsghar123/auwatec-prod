{
    "name": "she/ignore-trailing-slash",
    "description": "Ignore trailing slash",
    "type": "shopware-platform-plugin",
    "license": "proprietary",
    "version": "2.1.0",
    "authors": [
        {
            "name": "blackpoint GmbH",
            "homepage": "https://www.blackpoint.de"
        }
      ],
    "require": {
        "shopware/core": "^6.4"
    },
    "extra": {
        "shopware-plugin-class": "She\\IgnoreTrailingSlash\\SheIgnoreTrailingSlash",
        "label": {
            "de-DE": "Ignoriere Trailing Slash",
            "en-GB": "Ignore trailing slash"
