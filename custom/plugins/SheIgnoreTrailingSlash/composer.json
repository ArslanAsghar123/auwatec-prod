        "manufacturerLink": {
            "de-DE": "https://store.shopware.com/laudert.html",
            "en-GB": "https://store.shopware.com/laudert.html"
        },
        "supportLink": {
            "de-DE": "https://account.shopware.com/",
            "en-GB": "https://account.shopware.com/"
        }
    },
    "autoload": {
        "psr-4": {
            "She\\IgnoreTrailingSlash\\": "src/"
        }
    }
}
